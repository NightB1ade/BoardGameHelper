---
layout: empty
---

[
	{% for entry in site.data.AstroKnights -%}
	{% if forloop.first == false %}
	, {% endif -%}
	{
		"name": "{{ entry.name }}"
		, "knights": [
			{%- for knight in entry.knights -%}
			{% if forloop.first == false %}
			, {% endif -%}
			{
				"name": "{{ knight.name }}"
			}
			{%- endfor %}
		]
		, "homeworlds": [
			{%- for homeworld in entry.homeworlds -%}
			{% if forloop.first == false %}
			, {% endif -%}
			{
				"name": "{{ homeworld.name }}"
			}
			{%- endfor %}
		]
		, "bosses": [
			{%- for boss in entry.bosses -%}
			{% if forloop.first == false %}
			, {% endif -%}
			{
				"name": "{{ boss.name }}"
			}
			{%- endfor %}
		]
	}
	{%- endfor %}
]
